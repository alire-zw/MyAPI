{
  "name": "alireza-api",
  "version": "1.0.0",
  "description": "Database web service with MySQL and Node.js",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "init-db": "node database/init.js",
    "dev:frontend": "cd frontend; npm run dev",
    "dev:python": "cd apis/Fragment; python main.py",
    "dev:full": "concurrently \"npm run dev\" \"npm run dev:frontend\" \"npm run dev:python\"",
    "install:frontend": "cd frontend; npm install",
    "install:python": "cd apis/Fragment; pip install -r requirements.txt"
  },
  "dependencies": {
    "axios": "^1.11.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "mysql2": "^3.6.5"
  },
  "devDependencies": {
    "concurrently": "^9.2.0",
    "nodemon": "^3.0.2"
  },
  "keywords": [
    "api",
    "mysql",
    "database",
    "webservice"
  ],
  "author": "Alireza",
  "license": "MIT"
}
